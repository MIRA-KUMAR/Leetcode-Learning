def combinationSum(candidates, target):

    res = []

    def dfs(i, cur, total):
        if total == target:
            res.append(cur.copy())
            return

        if i >= len(candidates) or total > target:
            return

        cur.append(candidates[i])
        dfs(i, cur, sum(cur))
        cur.pop()
        dfs(i + 1, cur, sum(cur))

    dfs(0, [], 0)
    return res


print(combinationSum([2, 3, 6, 7], 7))
